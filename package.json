  "keywords": [
    "react",
    "d3",
    "vite",
    "tailwindcss",
    "typescript",
    "framer-motion",
    "data-visualization",
    "dashboard"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "npx eslint . --max-warnings=0",
    "tsc": "npx tsc --noEmit --pretty false --strict --noUnusedLocals --noUnusedParameters --noFallthroughCasesInSwitch --noImplicitOverride --noImplicitReturns --exactOptionalPropertyTypes",
    "prettier": "npx prettier . --write",
    "check": "npm run tsc && npm run lint",
    "test": "npx playwright test",
    "test:headed": "npx playwright test --headed",
    "test:report": "npx playwright show-report",
    "contents": "scripts/project_contents.sh"
  },
  "dependencies": {
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
